#include <iostream>

using namespace std;

int main()
{   
    int days;
    cout << "一號是星期幾(阿拉伯數字)";
    cin >> days;
    cout << "一 二 三 四 五 六 日\n";
    for(int i = 1; i < days; i++){
        cout << "  ";
    }
    for(int n = 0; n <= 7 - days; n++){
        cout << " " << n + 1<< " " ;
    }    
    return 0;
}